<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox style="-fx-background-color: #d9d9ff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox style="-fx-border-color: #3687ce; -fx-border-width: 0 0 3 0;" VBox.vgrow="ALWAYS">
         <children>
            <StackPane fx:id="mainPane" minHeight="550.0" minWidth="600.0" style="-fx-background-color: #44a0f2;" HBox.hgrow="ALWAYS">
               <children>
                  <StackPane fx:id="overlayPane" style="-fx-background-color: rgba(0, 100, 100, 0.25);">
                     <children>
                        <StackPane maxHeight="0.0" maxWidth="0.0" style="-fx-border-color: #3687ce; -fx-border-width: 3; -fx-background-color: #44a0f2;" StackPane.alignment="CENTER">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Victory!">
                                 <StackPane.margin>
                                    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                                 </StackPane.margin>
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font>
                              </Text>
                           </children>
                        </StackPane>
                     </children>
                  </StackPane>
               </children></StackPane>
            <StackPane minHeight="550.0" minWidth="200.0" style="-fx-background-color: #70b6f5;" HBox.hgrow="ALWAYS">
               <children>
                  <VBox>
                     <children>
                        <Pane VBox.vgrow="ALWAYS" />
                        <StackPane VBox.vgrow="NEVER">
                           <children>
                              <GridPane>
                                 <children>
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="[2]" GridPane.rowIndex="1" />
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="Reset map" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="[ESC]" GridPane.rowIndex="2" />
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="Return to menu" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="[1]" />
                                    <Text fill="#1b0075" strokeType="OUTSIDE" strokeWidth="0.0" text="Change map" GridPane.columnIndex="2" />
                                 </children>
                                 <columnConstraints>
                                    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
                                    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
                                    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                              </GridPane>
                           </children>
                        </StackPane>
                        <Pane VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </children>
            </StackPane>
         </children>
      </HBox>
      <HBox VBox.vgrow="NEVER">
         <children>
            <StackPane alignment="CENTER_LEFT" minWidth="-Infinity" HBox.hgrow="ALWAYS">
               <children>
                  <Text fx:id="playerMovesCountText" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of moves: 0" />
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" minWidth="-Infinity" HBox.hgrow="ALWAYS">
               <children>
                  <Text fx:id="boxesMovesCountText" strokeType="OUTSIDE" strokeWidth="0.0" text="Box move count: 0" />
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT" layoutX="378.0" layoutY="10.0" minWidth="-Infinity" HBox.hgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" " />
               </children>
            </StackPane>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
